{
  "name": "weather-app",
  "version": "1.0.2",
  "description": "An isomorphic React app that tells you what the weather is today.",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel && webpack --config=webpack.config.build.js",
    "test": "NODE_ENV=test mocha --recursive --require @babel/register --require @babel/polyfill",
    "test:ci": "NODE_ENV=test mocha --recursive --require @babel/register --require @babel/polyfill --reporter mocha-junit-reporter",
    "start:server": "webpack --mode=development --config=webpack.config.server.js",
    "start:client": "webpack-dev-server --mode=development --config=webpack.config.client.js",
    "start": "npm-run-all --parallel start:server start:client",
    "pretest": "NODE_ENV=test babel-node ./scripts/db/migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradhe/weather-app.git"
  },
  "author": "Brad Heller <brad.heller@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bradhe/weather-app/issues"
  },
  "homepage": "https://github.com/bradhe/weather-app#readme",
  "dependencies": {
    "@babel/core": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "axios": "^0.18.1",
    "babel-loader": "^8.0.6",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "css-loader": "^2.1.1",
    "debounce": "^1.2.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "guardtower": "^0.2.7",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "mustache": "^3.1.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.14",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-sortable-hoc": "^1.10.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.3.1",
    "start-server-webpack-plugin": "^2.2.5",
    "style-loader": "^0.23.1",
    "uuid": "^3.3.3",
    "webpack": "^4.41.0",
    "webpack-dev-server": "^3.8.2",
    "webpack-node-externals": "^1.7.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/node": "^7.6.2",
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "child-process-promise": "^2.2.1",
    "copy-webpack-plugin": "^5.0.4",
    "enzyme": "^3.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "mocha": "^6.2.1",
    "mocha-junit-reporter": "^1.23.1",
    "webpack-cli": "^3.3.9"
  }
}
